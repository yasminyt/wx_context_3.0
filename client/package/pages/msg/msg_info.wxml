    <view class="weui-msg__opr-area">
      <view class="weui-btn-area">
        <button bindtap='submit' class="weui-btn" type="primary">确 定</button>
        <button bindtap='reback' class="weui-btn">返 回</button>
      </view>
    </view>
  </view>
</view>